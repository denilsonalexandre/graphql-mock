{
  "Rota": {
    "id": { "int": { "min": 1000, "max": 1000000 } },
    "itinerarios": {
      "hasMany": "Itinerario"
    }
  },
  "Itinerario": {
    "id": { "int": { "min": 0, "max": 1000000 } },
    "sequencia": { "int": { "min": 0, "max": 1000000 } },
    "rota": {
      "belongsTo": "Rota"
    },
    "Endereco" : {
     "oneOf":"Endereco"
    }
  },
  "Endereco":{
    "id": { "int": { "min": 0, "max": 1000000 } },
    "enderecoCompleto":{
       "loremWords": { "minLength": 3, "maxLength": 8 }
    },
    "latitude":{
       "regex":"^(\\+|-)?(?:90(?:(?:\\.0{1,6})?)|(?:[0-9]|[1-8][0-9])(?:(?:\\.[0-9]{1,6})?))$"
    },
    "longitude":{
       "regex":"^(\\+|-)?(?:180(?:(?:\\.0{1,6})?)|(?:[0-9]|[1-9][0-9]|1[0-7][0-9])(?:(?:\\.[0-9]{1,6})?))$"
    }
  }
}
